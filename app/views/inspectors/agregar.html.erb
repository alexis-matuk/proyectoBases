
<%= flash[:error] %>

<h1>Lista Inspectores <%= link_to 'New Inspector', new_inspector_path, class: "btn btn-success pull-right" %></h1>


<div>

<div class="row">
   <div class="col-lg-12">


<table class="table table-striped table-advance table-hover">
  <tbody>
  <tr>
    <th><i class="icon_profile"></i>Nombre</th>
    <th><i class="icon_profile"></i>Institución</th>
    <th><i class="icon_mail_alt"></i> Departamento</th>
    <th><i class="icon_phone"></i> Teléfono</th>
  </tr>

    <% @inspectors.each do |inspector| %>
      <tr>
         <td><%= link_to inspector.nombre, llenar_establecimiento_inspector_path(ide: @establecimiento.id, id: inspector.id) %></td>
         <td><%= inspector.institucion%></td>
         <td><%= inspector.departamento%></td>
        <td><%= inspector.telefono %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>

<br>

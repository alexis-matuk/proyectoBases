<h1>Listing establecimiento_platillos</h1>

<table>
  <thead>
    <tr>
      <th>Introduccion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @establecimiento_platillos.each do |establecimiento_platillo| %>
      <tr>
        <td><%= establecimiento_platillo.introduccion %></td>
        <td><%= link_to 'Show', establecimiento_platillo %></td>
        <td><%= link_to 'Edit', edit_establecimiento_platillo_path(establecimiento_platillo) %></td>
        <td><%= link_to 'Destroy', establecimiento_platillo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

